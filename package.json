    "father": "^4.0.0-rc.2",
    "gh-pages": "^3.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^10.0.7",
    "sort-package-json": "^1.55.0",
    "typescript": "^4.7.4",
    "yorkie": "^2.0.0"
  },
